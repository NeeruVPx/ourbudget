                  <div class="input-group-btn">
                    <button type="button" class="btn btn-danger dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></button>
                    <ul id="urlList" class="dropdown-menu pull-right">
                      <li><a class = "dropdown-value">Action</a></li>
                      <li><a class = "dropdown-value">Another action</a></li>
                      <li><a class = "dropdown-value">Something else here</a></li>
                      <li><a class = "dropdown-value">Separated link</a></li>
                    </ul>
                  </div><!-- /btn-group -->
                </div><!-- /input-group -->
            </div>
            
          
                
               
            <div class = "col-md-3">
                <div id = "startDate" style="width : 88%;" class="input-append added-margin success date">
                        <input type="text" class="form-control">
                        <span class="add-on">
                            <span class="arrow"></span>
                            <i class="icon-th"></i>
                        </span>
                </div> 
            </div>
            <div class = "col-md-3">
                <div id = "endDate" style="width : 88%;" class="input-append added-margin success date">
                        <input type="text" class="form-control">
                        <span class="add-on">
                            <span class="arrow"></span>
                            <i class="icon-th"></i>
                        </span>
                </div> 
            </div>
            
           
        </div>
        
         <!-- Flot Chart CONTENT -->
        <div class="row">
        <div class="col-md-12">
          <div class="grid simple ">
            <div class="grid-title">
                <h4>Flot Chart</h4>
              <div class="tools"> <a href="javascript:;" class="collapse"></a> <a href="#grid-config" data-toggle="modal" class="config"></a> <a id = "flotRestore" class="reload"></a> 
             </div>
            </div>
            <div class="grid-body "> 
                <div id = "toggelSeriesDiv" class= "row">
                    <div class="row-fluid">
                    <div class="checkbox check-danger">
                      <input name = "impressions" id="impressionsToggle" type="checkbox" value="1" checked="checked">
                      <label for="impressionsToggle">Impressions</label>
                    </div>
                  </div>
                    
                    <div class="row-fluid">
                    <div class="checkbox check-success 	">
                      <input name = "earnings" id="earningsToggle" type="checkbox" value="1" checked="checked">
                      <label for="earningsToggle"> Earnings </label>
                    </div>
                  </div>
                    
                    <div class="row-fluid">
                    <div class="checkbox check-primary">
                      <input name = "ecpm" id="ecpmToggle" type="checkbox" value="1" checked="checked">
                      <label for="ecpmToggle">ECPM</label>
                    </div>
                  </div>
                    
                    
                </div>
                
               <div class="row spacing-bottom 2col">
                  <div class="col-md-4">
                    <div class="mini-chart-wrapper">
                      <div class="chart-details-wrapper">
                        <div class="chartname"> Impressions </div>
                        <div id = "totalImpressionsValue" class="chart-value"> 0 </div>
                      </div>
                      <div class="mini-chart">
                        <div id="mini-chart-orders"><canvas width="62" height="30" style="display: inline-block; width: 62px; height: 30px; vertical-align: top;"></canvas></div>
                      </div>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="mini-chart-wrapper">
                      <div class="chart-details-wrapper">
                        <div class="chartname"> E CPM </div>
                        <div id = "totalEcpmValue" class="chart-value"> $0 </div>
                      </div>
                      <div class="mini-chart">
                        <div id="mini-chart-other"><canvas width="62" height="30" style="display: inline-block; width: 62px; height: 30px; vertical-align: top;"></canvas></div>
                      </div>
                    </div>
                  </div>
                    <div class="col-md-4">
                    <div class="mini-chart-wrapper">
                      <div class="chart-details-wrapper">
                        <div class="chartname"> Earnings </div>
                        <div id = "totalEarningsValue" class="chart-value"> $0 </div>
                      </div>
                      <div class="mini-chart">
                        <div id="mini-chart-other"><canvas width="62" height="30" style="display: inline-block; width: 62px; height: 30px; vertical-align: top;"></canvas></div>
                      </div>
                    </div>
                  </div>
                </div>    
                
                <div id="placeholder" class="demo-placeholder" style="width:800px;height:300px;margin:0 auto">                </div>
                    
                
                </div>
            </div>
          </div>
        </div>
        <!-- Flot Chart End -->
        
         <!-- Data Chart CONTENT -->
        <div class="row">
        <div class="col-md-12">
          <div class="grid simple ">
            <div class="grid-title">
              <h4>Market <span class="semi-bold">Earnings</span></h4>
              <div class="tools"> <a href="javascript:;" class="collapse"></a> <a href="#grid-config" data-toggle="modal" class="config"></a> <a id = "tableController" class="reload"></a>  </div>
            </div>
            <div class="grid-body ">   
                <table class="table table-hover table-condensed" id="example">
                  <thead>
                    <tr>
                      <th width="1%"><div class="checkbox check-default" style="margin-right:auto;margin-left:auto;">
                          <input type="checkbox" value="1" id="checkbox1">
                          <label for="checkbox1"></label>
                        </div></th>
                      <th width="6%">AD SIZE</th>
                      <th width="6%" data-hide="phone,tablet">IMPRESSIONS</th>
                      <th width="6%">CTR</th>
                      <th width="10%" data-hide="phone,tablet">eCPM</th>
                       <th width="10%">ESTIMATED EARNINGS</th>
                    </tr>
                  </thead>
                  <tbody>
                                                         
                  </tbody>
                </table>
            </div>
          </div>
        </div>
      </div>
        <!-- Data Chart End -->
        
    </div>
         <!-- URL and Date Picker End -->
</div>
         
        
		<!-- END CONTENT -->
    </div>
  </div>
 </div>
