    <div class="content">  
		<div class="page-title">
            <h3><span class="semi-bold">Revenue Snapshot</span>
			</h3>		
		</div>
		<!-- START CONTAINER -->
	<div id="container">
		<div class="row spacing-bottom 2col">	
			<div id="todayDataTile"  class="col-md-3 col-sm-6 spacing-bottom-sm">	
				<div class="tiles blue added-margin">
				  <div class="tiles-body">
					<div class="controller">								
						<a href="javascript:refreshData('todayDataLabel');" class="reload"></a>	
					</div>		
					<div class="tiles-title">
						TODAY SO FAR
					</div>	
					<div class="heading">
					$ <span id = "todayDataLabel" class="animate-number" data-value="0" data-animation-duration="1200">0</span>
											
					</div>
                    <div>
					<div class="progress transparent progress-small no-radius">
						<div id = "todayDataProgress" class="progress-bar progress-bar-white animate-progress-bar" data-percentage="0%"></div>
					</div>
                    </div>
					</div>	
				</div>
			</div>
			<div id="yesterdayDataTile" class="col-md-3 col-sm-6 spacing-bottom-sm">
				<div class="tiles green added-margin">
				 <div class="tiles-body">
					<div class="controller">								
						<a href="javascript:refreshData('yesterdayDataLabel');" class="reload"></a>
                    </div>		
					<div class="tiles-title">
						YESTERDAY
					</div>	
					<div class="heading">
						$ <span id="yesterdayDataLabel" class="animate-number" data-value="0" data-animation-duration="1000">0</span>	
					</div>
								
					<div class="description"><i id="up-down" class="icon-custom-up icon-2x"></i><span id = "yesterdayDataProgress" class="text-white mini-description ">&nbsp; 2% higher <span class="blend">than last the day before</span></span></div>	
				 </div>
				</div>
			</div>
			<div id="curMonthDataTile" class="col-md-3 col-sm-6">
				<div class="tiles red added-margin">
				<div class="tiles-body">
					<div class="controller">								
							<a href="javascript:refreshData('monthDataLabel');" class="reload"></a>
				    </div>	
					<div class="tiles-title">
						THIS MONTH SO FAR
					</div>	
					<div class="heading">
						$ <span id="monthDataLabel" class="animate-number" data-value="0" data-animation-duration="1200">0</span>	
					</div>
					<div class="progress transparent progress-white progress-small no-radius">
						<div id = 'monthDataProgress' class="progress-bar progress-bar-white animate-progress-bar" data-percentage="0%" ></div>
					</div>
				</div>
				</div>
		
			</div> 
			<div id="lastMonthDataTile" class="col-md-3 col-sm-6">
				<div class="tiles purple added-margin">
				  <div class="tiles-body">
					<div class="controller">								
						<a href="javascript:refreshData('lastmonDataLabel');" class="reload"></a>
                    </div>		
					<div class="tiles-title">
						LAST MONTH
					</div>	
					<div class="row-fluid">
						<div class="heading">
							$ <span id="lastmonDataLabel" class="animate-number" data-value="0" data-animation-duration="700">0</span>	
						</div>
						<div  class="">
							Finalized Balance: $ 200
						</div>
					</div>
				  </div>
				</div>
			</div>	
      	</div>
	
       <div class = "row">
           <div class = "col-md-6">
               U.S Pacific Timezone, data is 2 hours delayed.
           </div>
